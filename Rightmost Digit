#include<stdio.h>
#include<math.h>
int main() {
    int i,k,n;
    int a[20] = { 0, 1, 4, 7, 6, 5, 6, 3, 6, 9, 0, 1, 6, 3, 6, 5, 6, 7, 4, 9 };
    while (scanf("%d", &n) != EOF) {
        for (i = 0; i < n; i++) {
            scanf("%d", &k);
            k = k % 20;
            printf("%d\n", a[k]);
        }
    }
    return 0;
}
